{
  "name": "@musallam/n8n-nodes-firefly-services",
  "version": "1.3.0",
  "description": "n8n community node for Adobe Firefly Services, Photoshop API, and Substance 3D - Generate images, videos, 3D content, and edit photos using Adobe's generative AI and professional editing tools",
  "license": "MIT",
  "homepage": "https://github.com/ahmed-musallam/n8n-nodes-firefly-services",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "test": "echo 'No tests yet'",
    "commit": "cz",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FireflyServicesApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AwsS3PresignedUrl/AwsS3PresignedUrl.node.js",
      "dist/nodes/FireflyAudioVideo/FireflyAudioVideo.node.js",
      "dist/nodes/FireflyServices/FireflyServices.node.js",
      "dist/nodes/JimpImageProcessing/JimpImageProcessing.node.js",
      "dist/nodes/Parse3D/Parse3D.node.js",
      "dist/nodes/Photoshop/Photoshop.node.js",
      "dist/nodes/Substance/Substance.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "adobe",
    "firefly",
    "photoshop",
    "substance-3d",
    "aws",
    "s3",
    "presigned-url",
    "jimp",
    "image-processing",
    "image-resize",
    "ai",
    "generative-ai",
    "image-generation",
    "video-generation",
    "3d-generation",
    "3d-rendering",
    "image-editing",
    "photo-editing",
    "background-removal",
    "masking",
    "generative-fill",
    "smart-objects",
    "text-to-image",
    "text-to-video",
    "text-to-speech",
    "avatar-generation",
    "video-reframing",
    "transcription",
    "dubbing",
    "lip-sync"
  ],
  "author": {
    "name": "Ahmed",
    "email": "musallam@adobe.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@n8n/node-cli": "*",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.0.0",
    "@types/node": "^24.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.32.0",
    "husky": "^9.1.7",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "semantic-release": "^25.0.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.1",
    "jimp": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
